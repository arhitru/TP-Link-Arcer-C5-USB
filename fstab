
config global
	option anon_swap '0'
	option anon_mount '0'
	option auto_swap '1'
	option auto_mount '1'
	option delay_root '5'
	option check_fs '0'

config mount 'extroot'
	option device '/dev/sda1'
	option target '/overlay'
	option enabled '1'

config mount 'rwm'
	option device '/dev/mtdblock4'
	option target '/rwm'

